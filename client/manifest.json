{
  "name": "ProductHunt-Instant",
  "description" : "Instant Notifier for new products on ProductHunt.com",
  "version": "1.0",
  "manifest_version": 2,
  "background":{
    "scripts": ["background.js"]
  },
  "content_security_policy" : "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": ["notifications"]
}
